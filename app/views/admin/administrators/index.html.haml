%h1 
  Administrators
  = link_to 'NEW ADMINISTRATOR', new_admin_administrator_url, class: 'btn'

%table

  %thead
    %tr
      %th Name
      %th E-mail
      %th
      
  %tbody
    - @administrators.each do |admin|
      %tr
        %td= admin.name
        %td= admin.email
        %td
          = link_to 'Show', admin_administrator_url(admin)
          = link_to 'Edit', edit_admin_administrator_url(admin)
