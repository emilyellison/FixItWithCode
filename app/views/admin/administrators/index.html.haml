%h1 Administrators

%table

  %thead
    %tr
      %th Name
      %th E-mail
      %th
      
  %tbody
    - @administrators.each do |admin|
      %tr
        %td= admin.name
        %td= admin.email
        %td
          = link_to 'Show', admin_administrator_url(admin)
          = link_to 'Edit', edit_admin_administrator_url(admin)
          = link_to 'Destroy', admin_administrator_url(admin), method: :delete, data: { confirm: 'Are you sure you want to delete this administrator???' }