- @administrators.each do |admin|
  .wrapper.left.top.small.white
    = image_tag "#{admin.first_name.downcase}.jpeg", width: '100%'

    %h3= link_to admin.name, administrator_url(first_name: admin.first_name)

    %p.small.icon
      = telephone_icon 24
      = phone_number_link admin.phone

    %p.small.icon
      = email_icon 24
      = mail_to admin.email, admin.email, subject: 'Hi'
      
    %p.small.icon
      = link_to github_icon(28), admin.github
      = link_to linked_in_icon(28), admin.linked_in
      = link_to twitter_icon(28), admin.twitter